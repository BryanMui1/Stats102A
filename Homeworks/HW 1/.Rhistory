reorder_cols <- function(input_df) {
col_order <- read.csv("TrackmanColumnOrder.csv", header = TRUE)$Columns
for (col in col_order) {
if (!(col %in% colnames(input_df))) {
input_df$col <- ""
}
}
input_df[, col_order]
}
